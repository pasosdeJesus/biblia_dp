<wi type="G" value="3682,2,">Onésimo</wi>,
<wi type="G" value="80,7,">el hermano</wi>
<wi type="G" value="3588,3,"/><wi type="G" value="4103,4,">fiel</wi>
<wi type="G" value="2532,5,">y</wi>
<wi type="G" value="27,6,">amado</wi>,
