<wi type="G" value="4061,9,">la circuncisión</wi>.
<wi type="G" value="3778,10,">Estos</wi> son mis
<wi type="G" value="3441,11,">únicos</wi>
<wi type="G" value="4904,12,">compañeros trabajadores</wi>
<wi type="G" value="1519,13,">para</wi>
<wi type="G" value="3588,14,"><wi type="G" value="932,15,">el Reino</wi></wi>
<wi type="G" value="3588,16,"><wi type="G" value="2316,17,">de Dios</wi></wi>,
