<wi type="G" value="3450,4,">mi</wi>
<wi type="G" value="3588,2,"><wi type="G" value="3986,3,">tentación</wi></wi>
<wi type="G" value="3588,5,">que</wi>
estaba <wi type="G" value="1722,6,">en</wi>
<wi type="G" value="3450,9,">mi</wi>
