<wi type="G" value="1473,7,">Yo</wi>
<wi type="G" value="2064,12,">vine</wi>
<wi type="G" value="1519,8,">a</wi>
<wi type="G" value="5126,11,">este</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2889,10,">mundo</wi></wi>,
<wi type="G" value="2443,13,">para que</wi>
<wi type="G" value="3588,14,">aquellos</wi> que
<wi type="G" value="3361,15,">no</wi>
<wi type="G" value="991,16,">ven</wi>
<wi type="G" value="991,17,">vean</wi>;
<wi type="G" value="2532,18,">y</wi> que
