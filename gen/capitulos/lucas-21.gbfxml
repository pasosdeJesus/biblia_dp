<wi type="G" value="3004,4,">diciendo</wi>,
«<wi type="G" value="1320,5,">Maestro</wi>,
<wi type="G" value="3767,7,">entonces</wi>
¿<wi type="G" value="4219,6,">cuándo</wi>
<wi type="G" value="2071,9,">serán</wi>
<wi type="G" value="5023,8,">estas cosas</wi>?
<wi type="G" value="2532,10,">Y</wi>
¿<wi type="G" value="5101,11,">cuál</wi> será
<wi type="G" value="3588,12,"><wi type="G" value="4592,13,">la señal</wi></wi>
<wi type="G" value="3752,14,">de que</wi>
<wi type="G" value="5023,16,">las cosas</wi>
<wi type="G" value="3195,15,">están por</wi>
<wi type="G" value="1096,17,">ocurrir</wi>?» </t>

