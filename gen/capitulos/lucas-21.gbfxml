<wi type="G" value="3588,8,"><wi type="G" value="5495,9,">manos</wi></wi>
<wi type="G" value="1909,6,">sobre</wi>
<wi type="G" value="5209,7,">ustedes</wi>
<wi type="G" value="2532,11,">y</wi> los
<wi type="G" value="1377,12,">perseguirán</wi>,
<wi type="G" value="3860,13,">enviándolos</wi>
<wi type="G" value="1519,14,">a</wi>
<wi type="G" value="4864,15,">las sinagogas</wi>
<wi type="G" value="2532,16,">y</wi>
a <wi type="G" value="5438,17,">las prisiones</wi>,
<wi type="G" value="71,18,">llevándolos</wi>
<wi type="G" value="1909,19,">frente a</wi>
<wi type="G" value="935,20,">reyes</wi>
<wi type="G" value="2532,21,">y</wi>
<wi type="G" value="2232,22,">gobernantes</wi>
