<wi type="G" value="2532,5,">y</wi>
<wi type="G" value="181,6,">disturbios</wi>,
<wi type="G" value="3361,7,">no</wi>
<wi type="G" value="4422,8,">se aterroricen</wi>,
<wi type="G" value="1063,10,">porque</wi>
<wi type="G" value="5023,11,">estas cosas</wi>
<wi type="G" value="1163,9,">deben</wi>
<wi type="G" value="1096,12,">pasar</wi>
<wi type="G" value="4412,13,">primero</wi>,
<wi type="G" value="235,14,">pero</wi>
<wi type="G" value="3588,17,"><wi type="G" value="5056,18,">el fin</wi></wi>
