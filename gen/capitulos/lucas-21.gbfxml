<wi type="G" value="3361,5,">no</wi>
<wi type="G" value="4105,6,">ser engañados</wi>,
<wi type="G" value="1063,8,">porque</wi>
<wi type="G" value="4183,7,">muchos</wi>
<wi type="G" value="2064,9,">vendrán</wi>
