<wi type="G" value="5503,7,">viudas</wi>,
que <wi type="G" value="846,9,">las</wi>
<wi type="G" value="1884,8,">ayude</wi>,
<wi type="G" value="2532,10,">y</wi> que
<wi type="G" value="3361,11,">no</wi>
