<wi type="G" value="2094,5,">años de edad</wi>,
<wi type="G" value="1096,7,">habiendo sido</wi>
<wi type="G" value="1135,10,">esposa</wi>
<wi type="G" value="1520,8,">de un</wi>
<wi type="G" value="435,9,">hombre</wi>,</t>
</sv>

<sv id="1_Timoteo-5-10">
