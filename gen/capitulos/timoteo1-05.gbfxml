<wi type="G" value="1487,7,"/><wi type="G" value="3361,8,"/><wi type="G" value="1909,9,">por</wi>
<wi type="G" value="1417,10,">dos</wi>
<wi type="G" value="2228,11,">o</wi>
<wi type="G" value="5140,12,">tres</wi>
