<wi type="G" value="4245,4,">los ancianos</wi>
<wi type="G" value="4291,3,">que gobiernen</wi>
<wi type="G" value="2573,2,">bien</wi>
<wi type="G" value="515,7,">sean contados como dignos</wi>
<wi type="G" value="1362,5,">de doble</wi>
<wi type="G" value="5092,6,">honor</wi>,
<wi type="G" value="3122,8,">especialmente</wi>
<wi type="G" value="3588,9,"/><wi type="G" value="2872,10,">los que trabajan</wi>
<wi type="G" value="1722,11,">en</wi>
<wi type="G" value="3056,12,">la palabra</wi>
