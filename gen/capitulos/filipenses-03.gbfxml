<wi type="G" value="3739,5,">cuyo</wi>
<wi type="G" value="3588,6,"><wi type="G" value="2316,7,">dios</wi></wi> es
<wi type="G" value="3588,8,"><wi type="G" value="2836,9,">el estomago</wi></wi>,
<wi type="G" value="2532,10,">y</wi> cuya
<wi type="G" value="3588,11,"><wi type="G" value="1391,12,">gloria</wi></wi> está
<wi type="G" value="1722,13,">en</wi>
<wi type="G" value="846,16,">su</wi>
<wi type="G" value="3588,14,"><wi type="G" value="152,15,">vergüenza</wi></wi>,
