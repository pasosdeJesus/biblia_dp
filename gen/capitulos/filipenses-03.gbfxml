<wi type="G" value="1473,2,">yo</wi>
<wi type="G" value="3756,4,">no</wi>
<wi type="G" value="1683,3,">me</wi>
<wi type="G" value="3049,5,">cuento como</wi>
<wi type="G" value="2638,6,">alcanzado</wi>,
<wi type="G" value="1161,8,">pero</wi>
<wi type="G" value="3303,10,">en verdad</wi>
es <wi type="G" value="1520,7,">una cosa</wi>:
<wi type="G" value="1950,12,">olvidar</wi>
<wi type="G" value="3588,9,">lo que</wi>
está <wi type="G" value="3694,11,">atrás</wi>,
