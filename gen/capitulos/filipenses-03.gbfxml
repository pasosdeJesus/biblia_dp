<wi type="G" value="3568,8,">ahora</wi>
<wi type="G" value="3004,12,">lo digo</wi>
<wi type="G" value="2532,10,">incluso</wi>
<wi type="G" value="2799,11,">llorando</wi>,
<wi type="G" value="4043,3,">caminan</wi>
como
