<wi type="G" value="3739,13,">cuyo</wi>
<wi type="G" value="3588,14,"><wi type="G" value="5056,15,">final</wi></wi>
<wi type="G" value="2071,16,">será</wi>
<wi type="G" value="2596,17,">acorde con</wi>
<wi type="G" value="846,20,">sus</wi>
<wi type="G" value="3588,18,"><wi type="G" value="2041,19,">obras</wi></wi>.</t>
</sv></cm><cm>

