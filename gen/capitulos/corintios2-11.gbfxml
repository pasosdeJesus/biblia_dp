<wi type="G" value="1722,9,">con</wi>
<wi type="G" value="846,12,">su</wi>
<wi type="G" value="3588,10,"><wi type="G" value="3834,11,">astucia</wi></wi>,
<wi type="G" value="3779,13,">así</wi>
