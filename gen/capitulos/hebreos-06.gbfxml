<wi type="G" value="1519,13,">por</wi>
<wi type="G" value="3588,14,"><wi type="G" value="165,15,">siempre</wi></wi>
<wi type="G" value="2596,7,">en</wi>
<wi type="G" value="3588,8,"><wi type="G" value="5010,9,">el orden</wi></wi>
