<wi type="G" value="1223,8,">mediante</wi>
<wi type="G" value="4102,9,">fe</wi>
<wi type="G" value="2532,10,">y</wi>
<wi type="G" value="3115,11,">paciencia</wi>
<wi type="G" value="2816,12,">heredaron</wi>
