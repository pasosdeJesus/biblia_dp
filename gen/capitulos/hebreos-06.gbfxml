<wi type="G" value="2192,4,">tenemos</wi>
<wi type="G" value="5613,2,">como</wi>
<wi type="G" value="45,3,">una ancla</wi>
<wi type="G" value="3588,5,"><wi type="G" value="5590,6,">del alma</wi></wi>,
<wi type="G" value="5037,8,">tanto</wi>
<wi type="G" value="804,7,">segura</wi>
<wi type="G" value="2532,9,">como</wi>
<wi type="G" value="949,10,">firme</wi>
<wi type="G" value="2532,11,">y</wi>
<wi type="G" value="1525,12,">que entra</wi>
<wi type="G" value="1519,13,">hasta</wi>
<wi type="G" value="3588,14,"/><wi type="G" value="2082,15,">dentro</wi>
