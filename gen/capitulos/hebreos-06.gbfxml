<wi type="G" value="1731,16,">mostraron</wi>
<wi type="G" value="1519,17,">por</wi>
<wi type="G" value="846,20,">Su</wi>
<wi type="G" value="3588,18,"><wi type="G" value="3686,19,">nombre</wi></wi>,
<wi type="G" value="1247,21,">habiendo servidor a</wi>
