de <wi type="G" value="4198,9,">continuar</wi>
<wi type="G" value="5613,10,"/>
<wi type="G" value="1909,11,">hacía</wi>
<wi type="G" value="3588,12,"><wi type="G" value="2281,13,">el océano</wi></wi>,
