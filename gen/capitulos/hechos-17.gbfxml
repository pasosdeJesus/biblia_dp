<wi type="G" value="5037,10,"/>
<wi type="G" value="3972,11,">a Pablo</wi>
<wi type="G" value="2532,12,">y</wi>
<wi type="G" value="3588,13,"><wi type="G" value="4609,14,">a Silas</wi></wi>
