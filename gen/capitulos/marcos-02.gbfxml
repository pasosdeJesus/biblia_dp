<wi type="G" value="2250,7,">días</wi>,
<wi type="G" value="2532,8,"/>
<wi type="G" value="191,9,">se escuchó</wi>
<wi type="G" value="3754,10,">que</wi>
<wi type="G" value="2076,13,">estaba</wi>
<wi type="G" value="1519,11,">en</wi>
