<wi type="G" value="235,13,">pero</wi>
<wi type="G" value="3588,14,"/>
<wi type="G" value="1607,15,">lo que sale</wi>
<wi type="G" value="575,16,">de</wi> <wi type="G" value="846,17,">él</wi>
