<wi type="G" value="3956,6,">todo</wi>
<wi type="G" value="2573,5,">bien</wi>.
<wi type="G" value="2532,8,"/>
<wi type="G" value="4160,11,">Ha hecho</wi>
<wi type="G" value="3588,9,"/>
<wi type="G" value="2974,10,">que los sordos</wi>
<wi type="G" value="191,12,">oigan</wi>,
