<wi type="G" value="575,6,">de</wi>
<wi type="G" value="3588,7,"><wi type="G" value="3793,8,">la multitud</wi></wi> y <wi type="G" value="1525,3,">entró</wi>
<wi type="G" value="1519,4,">a</wi>
<wi type="G" value="3624,5,">la casa</wi>,
<wi type="G" value="846,13,">sus</wi>
<wi type="G" value="3588,11,"><wi type="G" value="3101,12,">discípulos</wi></wi>
