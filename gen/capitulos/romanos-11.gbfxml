<wi type="G" value="3004,1,">digo</wi>,
¿Acaso <wi type="G" value="3588,5,"><wi type="G" value="2316,6,">Dios</wi></wi>
<wi type="G" value="3361,3,"/><wi type="G" value="683,4,">expulsó</wi>
<wi type="G" value="846,9,">a Su</wi>
<wi type="G" value="3588,7,"><wi type="G" value="2992,8,">gente</wi></wi>?
¡<wi type="G" value="3361,10,"><wi type="G" value="1096,11,">De ninguna manera</wi></wi>!
<wi type="G" value="1063,13,">Pues</wi>
<wi type="G" value="1473,14,">yo</wi>
