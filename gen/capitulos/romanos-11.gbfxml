<wi type="G" value="2504,11,">yo</wi>
<wi type="G" value="5275,12,">quedé</wi>
<wi type="G" value="3441,13,">sólo</wi>,
<wi type="G" value="2532,14,">y</wi>
<wi type="G" value="2212,15,">ellos buscan</wi>
