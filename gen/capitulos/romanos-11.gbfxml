<wi type="G" value="5485,3,">por gracia</wi>,
<wi type="G" value="3765,4,">entonces ya no</wi> es
<wi type="G" value="1537,5,">por</wi>
<wi type="G" value="2041,6,">obras</wi>;
<wi type="G" value="1893,7,">de lo contrario</wi>
<wi type="G" value="3588,8,"><wi type="G" value="5485,9,">la gracia</wi></wi>
<wi type="G" value="3765,10,">ya no</wi>
<wi type="G" value="1096,11,">sería</wi>
<wi type="G" value="5485,12,">gracia</wi>.  </t>
<rb xml:lang="es"><wi type="G" value="1161,14,">Pero</wi>
