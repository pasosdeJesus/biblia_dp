<wi type="G" value="846,6,">Él</wi>
<wi type="G" value="3588,4,"/><wi type="G" value="1511,5,">estaba</wi>
<wi type="G" value="4336,10,">orando</wi>
<wi type="G" value="1722,7,">en</wi>
<wi type="G" value="5100,9,">cierto</wi>
<wi type="G" value="5117,8,">lugar</wi>,
<wi type="G" value="5613,11,">cuando</wi>
<wi type="G" value="3973,12,">terminó</wi>
<wi type="G" value="5100,14,">uno</wi> de
<wi type="G" value="846,17,">sus</wi>
<wi type="G" value="3588,15,"><wi type="G" value="3101,16,">discípulos</wi></wi>
<wi type="G" value="4314,18,"/><wi type="G" value="846,19,">le</wi>
<wi type="G" value="2036,13,">dijo</wi>,
«<wi type="G" value="2962,20,">Señor</wi>,
<wi type="G" value="1321,21,"><wi type="G" value="2248,22,">enséñanos</wi></wi>
