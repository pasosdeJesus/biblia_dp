<wi type="G" value="3588,4,"/><wi type="G" value="846,6,"><wi type="G" value="1316,5,">se alejaban</wi></wi>
<wi type="G" value="575,7,">de</wi>
<wi type="G" value="846,8,">Él</wi>, que
