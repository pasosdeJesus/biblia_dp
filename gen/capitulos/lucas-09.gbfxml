<wi type="G" value="4862,6,">con</wi>
<wi type="G" value="846,7,">Él</wi>
<wi type="G" value="3588,5,"><wi type="G" value="916,9,">estaban pesados</wi></wi> por
<wi type="G" value="5258,10,">el sueño</wi>,
<wi type="G" value="1161,12,">más</wi>
<wi type="G" value="1235,11,">cuando despertaron</wi>,
