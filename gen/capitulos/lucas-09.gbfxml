<wi type="G" value="2036,9,">le dijo</wi>
<wi type="G" value="4314,12,">a</wi>
<wi type="G" value="3588,13,"><wi type="G" value="2424,14,">Jesús</wi></wi>,
«<wi type="G" value="1988,15,">Maestro</wi>,
<wi type="G" value="2076,17,">es</wi>
<wi type="G" value="2570,16,">bueno</wi>
<wi type="G" value="2248,18,">para nosotros</wi>
<wi type="G" value="1511,20,">estar</wi>
<wi type="G" value="5602,19,">aquí</wi>.
<wi type="G" value="2532,21,"/><wi type="G" value="4160,22,">Hagamos</wi>
<wi type="G" value="5140,24,">tres</wi>
<wi type="G" value="4633,23,">tiendas</wi>:
<wi type="G" value="1520,25,">una</wi>
<wi type="G" value="4671,26,">para ti</wi>,
<wi type="G" value="2532,27,"/><wi type="G" value="1520,29,">una</wi>
<wi type="G" value="3475,28,">para Moisés</wi>
<wi type="G" value="2532,30,">y</wi>
<wi type="G" value="1520,31,">una</wi>
<wi type="G" value="2243,32,">para Elías</wi>»
<wi type="G" value="3361,33,">sin</wi>
<wi type="G" value="1492,34,">saber</wi>
<wi type="G" value="3739,35,">lo que</wi>
