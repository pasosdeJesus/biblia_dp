<wi type="G" value="3588,6,"/>
<wi type="G" value="3813,7,">niños</wi>,
<wi type="G" value="2532,8,"/>
<wi type="G" value="3361,9,">no</wi>
<wi type="G" value="846,11,">les</wi>
<wi type="G" value="2967,10,">prohíban</wi>
<wi type="G" value="2064,12,">venir</wi>
<wi type="G" value="4314,13,">a</wi>
<wi type="G" value="3165,14,">mí</wi>;
<wi type="G" value="3588,15,"/>
<wi type="G" value="1063,16,">porque</wi>
de </t>
