<wi type="G" value="169,25,">impuros</wi>,
<wi type="G" value="2532,27,">y</wi> ellos
<wi type="G" value="846,29,">le</wi>
<wi type="G" value="5219,28,">obedecen</wi>!» </t>
</sv>
