<wi type="G" value="2476,5,">se presenta</wi>
<wi type="G" value="2596,6,">a</wi>
<wi type="G" value="2250,7,">diario</wi>
<wi type="G" value="3008,8,">ministrando</wi>
<wi type="G" value="2532,9,">y</wi>
<wi type="G" value="4374,13,">ofreciendo</wi>
