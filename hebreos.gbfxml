<wi type="G" value="1093,1,">la tierra</wi>
<wi type="G" value="3588,3,">que</wi>
<wi type="G" value="4095,4,">bebió</wi>
<wi type="G" value="5205,10,">la lluvia</wi> que
<wi type="G" value="4178,8,">con frecuencia</wi>
<wi type="G" value="2064,9,">viene</wi>
<wi type="G" value="3588,5,"/><wi type="G" value="1909,6,">a</wi>
<wi type="G" value="846,7,">ella</wi>,
<wi type="G" value="2532,11,">y</wi>
<wi type="G" value="5088,12,">da</wi> </t>
