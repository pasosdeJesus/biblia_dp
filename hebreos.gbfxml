<wi type="G" value="530,11,">una vez</wi>
<wi type="G" value="1909,12,">al</wi>
<wi type="G" value="4930,13,">final</wi>
<wi type="G" value="3588,14,"><wi type="G" value="165,15,">de los tiempos</wi></wi>,
<wi type="G" value="5319,23,">ha sido revelado</wi>
<wi type="G" value="1519,16,">para</wi>
<wi type="G" value="115,17,">deshacer</wi>
<wi type="G" value="266,18,">el pecado</wi>
<wi type="G" value="1223,19,">por</wi>
<wi type="G" value="846,22,">su propio</wi>
