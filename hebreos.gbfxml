<wi type="G" value="1223,6,">porque</wi>
<wi type="G" value="3000,14,">los adoradores</wi>,
<wi type="G" value="530,15,">una vez</wi>
<wi type="G" value="2508,16,">limpios</wi>,
<wi type="G" value="2089,10,">ya</wi>
<wi type="G" value="3588,7,"/><wi type="G" value="3367,8,">no</wi>
<wi type="G" value="2192,9,">tendrían</wi>
<wi type="G" value="4893,11,">conciencia</wi>
<wi type="G" value="3588,13,"/><wi type="G" value="266,12,">del pecado</wi>?
</t>
</sv>
<sv id="Hebreos-10-3">
But in
