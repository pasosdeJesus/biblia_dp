que son <wi type="G" value="499,9,">representaciones</wi>
<wi type="G" value="3588,10,">del</wi>
<wi type="G" value="228,11,">verdadero</wi>,
<wi type="G" value="235,12,">sino</wi>
<wi type="G" value="1519,13,">en</wi>
