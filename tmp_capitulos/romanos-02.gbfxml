<wi type="G" value="3588,8,"><wi type="G" value="1108,9,">de conocimiento</wi></wi>
<wi type="G" value="2532,10,">y</wi>
<wi type="G" value="3588,11,"><wi type="G" value="225,12,">de verdad</wi></wi>
