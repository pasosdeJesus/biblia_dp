<wi type="G" value="3588,1,"/>
<wi type="G" value="749,3,">los jefes de los sacerdotes</wi>
<wi type="G" value="2532,4,">y</wi>
<wi type="G" value="3588,5,"/>
<wi type="G" value="4245,6,">los ancianos</wi>
<wi type="G" value="3982,7,">persuadieron</wi>
<wi type="G" value="3588,8,"/>
<wi type="G" value="3793,9,">a la multitud</wi>
<wi type="G" value="2443,10,">para que</wi>
<wi type="G" value="154,11,">pidieran</wi>
<wi type="G" value="3588,12,"/>a
<wi type="G" value="912,13,">Barrabás</wi>
