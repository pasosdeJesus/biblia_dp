<wi type="G" value="1161,2,"/>
<wi type="G" value="1859,3,">la fiesta</wi>
<wi type="G" value="3588,5,"/><wi type="G" value="2232,6,">el gobernador</wi>
<wi type="G" value="1486,4,">solía</wi>
<wi type="G" value="630,7,">liberar</wi>
<wi type="G" value="1198,11,">un prisionero</wi>
