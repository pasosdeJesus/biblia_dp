<wi type="G" value="3756,2,">no</wi>
<wi type="G" value="4314,5,"/>
<wi type="G" value="846,4,">le</wi> <wi type="GC" value="3">respondió</wi>,
<wi type="G" value="3761,6,">ni</wi> siquiera
<wi type="G" value="1520,7,">una</wi>
<wi type="G" value="4487,8,">palabra</wi>,
<wi type="G" value="5620,9,">así</wi> que
<wi type="G" value="3588,11,"/>
<wi type="G" value="2232,12,">el gobernador</wi>
<wi type="G" value="2296,10,">se maravilló</wi>
<wi type="G" value="3029,13,">grandemente</wi>. </t>
</sv>
