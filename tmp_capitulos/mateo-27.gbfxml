<wi type="G" value="3588,10,"/>
<wi type="G" value="1135,11,">esposa</wi>
<wi type="G" value="4314,8,"><wi type="G" value="846,9,">le</wi></wi>
<wi type="G" value="649,7,">envió</wi>,
<wi type="G" value="3004,13,">diciendo</wi>,
«<wi type="G" value="3367,14,"><wi type="G" value="4671,15,">No tengas</wi></wi> nada
<wi type="G" value="2532,16,">con</wi>
<wi type="G" value="3588,17,"/>
<wi type="G" value="1565,19,">este</wi>
<wi type="G" value="1342,18,">hombre justo</wi>,
