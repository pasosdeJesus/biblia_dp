<wi type="G" value="2521,1,"><wi type="G" value="846,3,">se sentaba</wi></wi>
<wi type="G" value="1909,4,">en</wi> <wi type="G" value="3588,5,"/>
<wi type="G" value="968,6,">la silla para juzgar</wi>,
