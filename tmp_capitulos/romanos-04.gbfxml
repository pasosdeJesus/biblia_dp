<wi type="G" value="2818,6,">herederos</wi>,
<wi type="G" value="3588,8,"><wi type="G" value="4102,9,">la fe</wi></wi>
<wi type="G" value="2758,7,">se hace vacía</wi>,
<wi type="G" value="2532,10,">y</wi>
<wi type="G" value="3588,12,"><wi type="G" value="1860,13,">la promesa</wi></wi>
