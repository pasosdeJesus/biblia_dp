<wi type="G" value="4973,5,">un sello</wi>
<wi type="G" value="3588,6,"><wi type="G" value="1343,7,">de la
justicia</wi></wi>
<wi type="G" value="3588,8,"><wi type="G" value="4102,9,">de la fe</wi></wi>
