<rb xml:lang="es"><wi type="G" value="3588,19,">lo que</wi>
<wi type="G" value="3361,20,">no</wi>
<wi type="G" value="5607,21,">es</wi>
<wi type="G" value="5613,22,">para que</wi>
<wi type="G" value="5607,23,">sea</wi><rf><citebib id="SE"/> dice
