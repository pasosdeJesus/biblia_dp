<wi type="G" value="3588,20,"><wi type="G" value="3551,21,">la ley</wi></wi>,
<wi type="G" value="235,23,">sino</wi>
<wi type="G" value="2532,24,">también</wi>
<wi type="G" value="3588,25,">para el que</wi>
<wi type="G" value="1537,26,">es de</wi>
<wi type="G" value="4102,27,">la fe</wi>
<wi type="G" value="11,28,">de Abraham</wi>,
<wi type="G" value="3739,29,">quien</wi>
<wi type="G" value="2076,30,">es</wi>
<wi type="G" value="3962,31,">el padre</wi>
