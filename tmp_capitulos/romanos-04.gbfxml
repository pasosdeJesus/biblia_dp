<wi type="G" value="1909,4,">en</wi>
<wi type="G" value="1680,5,">la esperanza</wi>,
<wi type="G" value="1519,7,">para</wi>
<wi type="G" value="3588,8,"/><wi type="G" value="1096,9,">poder ser</wi>
