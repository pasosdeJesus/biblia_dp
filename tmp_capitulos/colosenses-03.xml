<wi type="G" value="1722,7,">en</wi>
<wi type="G" value="5216,10,">sus</wi>
<wi type="G" value="3588,8,"><wi type="G" value="2588,9,">corazones</wi></wi>,
<wi type="G" value="1519,11,">a</wi>
<wi type="G" value="3739,12,">la cual</wi>
<wi type="G" value="2532,13,">también</wi>
