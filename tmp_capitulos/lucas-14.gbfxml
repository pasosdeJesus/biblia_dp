<wi type="G" value="1096,2,">ocurrió</wi>
<wi type="G" value="1722,3,">que</wi>
<wi type="G" value="846,6,">Él</wi>
<wi type="G" value="3588,4,"/><wi type="G" value="2064,5,">fue</wi>
<wi type="G" value="1519,7,">a</wi>
<wi type="G" value="3624,8,">la casa</wi>
<wi type="G" value="5100,9,">de uno</wi>
<wi type="G" value="3588,10,"><wi type="G" value="758,11,">de los jefes</wi></wi>
