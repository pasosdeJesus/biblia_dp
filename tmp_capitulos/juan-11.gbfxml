<wi type="G" value="305,9,">salieron</wi>
<wi type="G" value="1537,13,">de</wi>
<wi type="G" value="3588,14,"><wi type="G" value="5561,15,">los campos</wi></wi>
<wi type="G" value="1519,11,">a</wi>
<wi type="G" value="2414,12,">Jerusalén</wi>
