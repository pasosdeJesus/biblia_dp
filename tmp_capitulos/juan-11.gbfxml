<t xml:lang="es"><wi type="G" value="1161,2,">Y</wi>
<wi type="G" value="2258,1,">estaba</wi>
<wi type="G" value="1451,3,">cerca</wi>
<wi type="G" value="3588,4,"/><wi type="G" value="3957,5,">la pascua</wi>
