<wi type="G" value="575,8,">de</wi>
<wi type="G" value="846,9,">él</wi>;
<wi type="G" value="3754,10,">porque</wi>
<wi type="G" value="3756,11,">no</wi>
<wi type="G" value="1492,12,">conocen</wi>
