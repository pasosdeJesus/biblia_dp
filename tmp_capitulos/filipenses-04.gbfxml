<wi type="G" value="2257,6,">nuestro</wi>
sea <wi type="G" value="3588,7,"><wi type="G" value="1391,8,">la gloria</wi></wi>
<wi type="G" value="1519,9,">por</wi>
<wi type="G" value="3588,10,"><wi type="G" value="165,11,">siempre</wi></wi>
y
