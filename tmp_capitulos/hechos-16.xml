<wi type="G" value="4862,5,">con</wi>
<wi type="G" value="846,6,">él</wi>,
<wi type="G" value="2532,8,"/>
<wi type="G" value="846,11,">lo</wi>
<wi type="G" value="2983,9,">tomó</wi>
<wi type="G" value="4059,10,">y lo circuncidó</wi>
