<wi type="G" value="2532,5,">y</wi>
<wi type="G" value="3082,6,">Listra</wi>.
<wi type="G" value="2532,7,"/>
<wi type="G" value="2400,8,">Observe que</wi>
<wi type="G" value="5100,10,">cierto</wi>
<wi type="G" value="3101,9,">discípulo</wi>
<wi type="G" value="3686,13,">de nombre</wi>
<wi type="G" value="5095,14,">Timoteo</wi>
<wi type="G" value="2258,11,">estaba</wi>
