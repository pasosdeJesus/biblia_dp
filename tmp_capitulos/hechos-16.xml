<wi type="G" value="5442,8,">por guardar</wi>
<wi type="G" value="3588,11,"/>
<wi type="G" value="2919,12,">que habían sido ordenados</wi>
<wi type="G" value="5259,13,">por</wi>
<wi type="G" value="3588,14,"><wi type="G" value="652,15,">los apóstoles</wi></wi>
