<wi type="G" value="2872,5,">trabajamos</wi>
<wi type="G" value="2532,6,">y</wi>
<wi type="G" value="3679,7,">sufrimos reproches</wi>,
<wi type="G" value="3754,8,">porque</wi>
<wi type="G" value="1679,9,">hemos confiado</wi>
<wi type="G" value="1909,10,">en</wi>
<wi type="G" value="2316,11,">el Dios</wi>
<wi type="G" value="2198,12,">vivo</wi>,
<wi type="G" value="3739,13,">quien</wi>
<wi type="G" value="2076,14,">es</wi>
<wi type="G" value="4990,15,">el Salvador</wi>
