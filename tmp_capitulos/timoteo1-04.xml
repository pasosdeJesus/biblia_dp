<wi type="G" value="3853,1,">ordena</wi>
<wi type="G" value="2532,3,">y</wi>
<wi type="G" value="1321,4,">enseña</wi>.
</t>

</sv></cm><cm>

<sv id="1_Timoteo-4-12">
