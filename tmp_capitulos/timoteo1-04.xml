<wi type="G" value="4314,5,"/><wi type="G" value="3641,6,">un poco</wi>
<wi type="G" value="5624,8,">de valor</wi>,
<wi type="G" value="1161,10,">pero</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2150,11,">la piedad</wi></wi>
<wi type="G" value="2076,15,">es</wi>
