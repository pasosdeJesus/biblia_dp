<wi type="G" value="1096,8,">se</wi>
<wi type="G" value="5179,7,">un ejemplo</wi> para
<wi type="G" value="3588,9,">los que</wi>
<wi type="G" value="4103,10,">creen</wi>
<wi type="G" value="1722,11,">en</wi>
<wi type="G" value="3056,12,">la palabra</wi>,
<wi type="G" value="1722,13,">con</wi> </t>
<rb xml:lang="es"><wi type="G" value="391,14,">tu forma de vivir</wi><rf>
