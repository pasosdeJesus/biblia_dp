<wi type="G" value="2228,8,">o</wi>
<wi type="G" value="4095,9,">beba</wi>
<wi type="G" value="3588,10,"><wi type="G" value="4221,11,">de la copa</wi></wi>
<wi type="G" value="3588,12,"><wi type="G" value="2962,13,">del Señor</wi></wi>
<wi type="G" value="371,14,">indignamente</wi>
<wi type="G" value="2071,16,">será</wi>
<wi type="G" value="1777,15,">culpable del</wi>
