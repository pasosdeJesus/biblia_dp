<t xml:lang="es">
<wi type="G" value="1063,2,">Pues</wi>
<wi type="G" value="3740,1,">siempre que</wi>
<wi type="G" value="302,3,"/><wi type="G" value="2068,4,">coman</wi>
<wi type="G" value="5126,7,">este</wi>
<wi type="G" value="3588,5,"><wi type="G" value="740,6,">pan</wi></wi>
<wi type="G" value="2532,8,">y</wi>
<wi type="G" value="4095,12,">beban</wi>
<wi type="G" value="5124,11,">de esta</wi>
<wi type="G" value="3588,9,"><wi type="G" value="4221,10,">copa</wi></wi>,
