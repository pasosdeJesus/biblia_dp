<wi type="G" value="3762,9,">no</wi>
<wi type="G" value="2071,10,">habrá</wi>
<wi type="G" value="580,6,">pérdidas</wi>,
de <wi type="G" value="5590,8,">vidas</wi>
<wi type="G" value="1537,11,">entre</wi>
<wi type="G" value="5216,12,">ustedes</wi>
<wi type="G" value="4143,13,">sino sólo</wi>
