<wi type="G" value="435,3,">señores</wi>,
<wi type="G" value="2114,2,">anímense</wi>!
<wi type="G" value="1063,5,">Pues</wi>
<wi type="G" value="4100,4,">creo</wi>
<wi type="G" value="3588,6,"><wi type="G" value="2316,7,">en Dios</wi></wi>,
<wi type="G" value="3754,8,">y que</wi>
<wi type="G" value="3779,9,"><wi type="G" value="2071,10,">ocurrirá</wi></wi>
<wi type="G" value="2596,11,">tal</wi>
<wi type="G" value="3739,12,"/>
<wi type="G" value="5158,13,">como</wi>
se
<wi type="G" value="3427,15,">me</wi>
