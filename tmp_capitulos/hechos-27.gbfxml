<wi type="G" value="5037,2,"/>
<wi type="G" value="3381,3,">que talvez</wi> </t>
<rb xml:lang="es">
<wi type="G" value="1601,7,">cayeramos</wi>
<wi type="G" value="1519,4,">sobre</wi>
<wi type="G" value="5138,5,">un lugar</wi>
<wi type="G" value="5117,6,">rocoso</wi><rf><citebib id="WEB"/> dice
