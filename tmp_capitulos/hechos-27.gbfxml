<wi type="G" value="1161,2,"/>
<wi type="G" value="2212,4,">intentaron</wi>
<wi type="G" value="5343,5,">huir</wi>
<wi type="G" value="1537,6,">del</wi>
<wi type="G" value="3588,7,"><wi type="G" value="4143,8,">barco</wi></wi>,
<wi type="G" value="2532,9,">y</wi> </t>
<rb xml:lang="es"><wi type="G" value="5465,10,">bajaron</wi>
<wi type="G" value="3588,11,">el</wi> <wi type="G" value="4627,12,">bote auxiliar</wi>
