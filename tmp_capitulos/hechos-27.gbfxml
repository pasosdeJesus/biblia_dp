`<wi type="G" value="3361,2,">No</wi>
<wi type="G" value="5399,3,">temas</wi>
<wi type="G" value="3972,4,">Pablo</wi>.
<wi type="G" value="4571,6,"><wi type="G" value="1163,7,">Debes</wi></wi>
<wi type="G" value="3936,8,">ser llevado ante</wi>
<wi type="G" value="2541,5,">el Cesar</wi>.
<wi type="G" value="2532,9,"/>
<wi type="G" value="2400,10,">Observa</wi>,
<wi type="G" value="3588,13,"><wi type="G" value="2316,14,">Dios</wi></wi>
<wi type="G" value="4671,12,">te</wi>
<wi type="G" value="5483,11,">ha concedido</wi>
<wi type="G" value="3956,15,">todos</wi>
<wi type="G" value="3588,16,">aquellos</wi>
<wi type="G" value="4126,17,">que zarparon</wi>
