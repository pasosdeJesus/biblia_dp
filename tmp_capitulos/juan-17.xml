<wi type="G" value="3739,4,">que</wi>
<wi type="G" value="3427,6,">me</wi>
<wi type="G" value="1325,7,">has dado</wi>
<wi type="G" value="2532,9,">y</wi>
<wi type="G" value="846,10,">ellos</wi> las
<wi type="G" value="2983,11,">recibieron</wi>,
<wi type="G" value="2532,12,">y</wi>
<wi type="G" value="1097,13,">han sabido</wi>
<wi type="G" value="230,14,">con certeza</wi>
<wi type="G" value="3754,15,">que</wi>
<wi type="G" value="1831,18,">Yo vengo</wi>
