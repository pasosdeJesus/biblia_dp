<wi type="G" value="3844,7,"><wi type="G" value="4572,8,">contigo mismo</wi></wi>,
<wi type="G" value="3588,9,"><wi type="G" value="1391,10,">con la gloria</wi></wi>
<wi type="G" value="3739,11,">que</wi>
<wi type="G" value="2192,12,">Yo tenía</wi>
<wi type="G" value="3844,18,"><wi type="G" value="4671,19,">contigo</wi></wi>
