<wi type="G" value="3588,5,"><wi type="G" value="1093,6,">la tierra</wi></wi>.
<wi type="G" value="5048,9,">He completado</wi>
<wi type="G" value="3588,7,"><wi type="G" value="2041,8,">la labor</wi></wi>
<wi type="G" value="3739,10,">que</wi>
<wi type="G" value="3427,12,">me</wi>
