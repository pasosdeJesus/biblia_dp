<wi type="G" value="1519,9,">contra</wi>
<wi type="G" value="3588,10,"><wi type="G" value="3101,11,">los discípulos</wi></wi>
<wi type="G" value="3588,12,"><wi type="G" value="2962,13,">del Señor</wi></wi>,
