<wi type="G" value="1488,6,">eres</wi>
<wi type="G" value="4771,4,">tu</wi>
<wi type="G" value="3588,7,"/><wi type="G" value="470,8,">para replicar ante</wi>
<wi type="G" value="3588,9,"><wi type="G" value="2316,10,">Dios</wi></wi>?
</t>
