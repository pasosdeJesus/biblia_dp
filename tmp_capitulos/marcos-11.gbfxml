<wi type="G" value="1448,3,">se acercaron</wi>
<wi type="G" value="1519,4,">a</wi>
<wi type="G" value="2419,5,">Jerusalén</wi>,
<wi type="G" value="1519,6,">en</wi>
<wi type="G" value="967,7,">Betfagé</wi>
<wi type="G" value="2532,8,">y</wi>
<wi type="G" value="963,9,">Betania</wi>,
