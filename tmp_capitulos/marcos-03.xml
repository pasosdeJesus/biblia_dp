<wi type="G" value="1831,2,">salieron</wi>, y
<wi type="G" value="2112,5,">de inmediato</wi>
<wi type="G" value="4160,10,">hicieron</wi>
<wi type="G" value="4824,9,">consulta</wi>
<wi type="G" value="3326,6,">con</wi>
<wi type="G" value="3588,7,"><wi type="G" value="2265,8,">los seguidores de Herodes</wi></wi>
<wi type="G" value="2596,11,">en contra</wi>
