<wi type="G" value="846,8,">ellos</wi>
<wi type="G" value="3361,6,">no</wi>
<wi type="G" value="1410,7,">podían</wi>
<wi type="G" value="3383,9,">ni</wi>
<wi type="G" value="5315,11,">comer</wi>
<wi type="G" value="740,10,">pan</wi>. </t>
</sv>
<sv id="Marcos-3-21">
