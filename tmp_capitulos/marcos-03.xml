<wi type="G" value="2532,9,"/> <wi type="G" value="2381,10,">Tomás</wi>;
<wi type="G" value="2532,11,"> <wi type="G" value="2385,12,">Santiago</wi></wi>
<wi type="G" value="3588,13,">el</wi> hijo
